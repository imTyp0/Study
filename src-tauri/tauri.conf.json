{
  "build": {
    "devPath": "../src/",
    "distDir": "../src/",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "study",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "windows": [
      {
        "title": "",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": "'unsafe-inline'"
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": ".deb",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
